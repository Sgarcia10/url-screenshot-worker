  "scripts": {
    "prebuild": "rimraf dist",
    "build": "webpack",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "start": "node dist/main.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test_e2e/**/*.ts\"",
    "test": "jest --config jest.config.json --forceExit --collect-coverage --detectOpenHandles --maxWorkers=1",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test_e2e/jest-e2e.json",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand",
    "migrate": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migrate:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert",
    "migrate:seed": "npm run migrate && npm run seed",
    "seed": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/config/orm.config.ts",
    "typeorm:migrate": "npm run typeorm migration:generate -- -n",
    "typeorm:run": "npm run typeorm migration:run"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@nestjs/common": "^6.7.2",
    "@nestjs/core": "^6.7.2",
    "@nestjs/event-emitter": "0.0.2",
    "@nestjs/platform-express": "^6.7.2",
    "@nestjs/swagger": "^4.5.5",
    "@nestjs/typeorm": "^7.1.3",
    "aws-sdk": "^2.799.0",
    "chalk": "^4.0.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "dotenv": "^8.2.0",
    "express-healthcheck": "^0.1.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.31",
    "morgan": "^1.10.0",
    "nock": "^13.0.4",
    "pg": "^8.3.3",
    "puppeteer": "^5.5.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "sqs-consumer": "^5.5.0",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.25",
    "typeorm-global-scopes": "^0.2.3",
    "typeorm-seeding": "^1.6.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@nestjs/cli": "^7.1.5",
    "@nestjs/schematics": "^6.7.0",
    "@nestjs/testing": "^6.7.1",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.5",
    "@types/supertest": "^2.0.8",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.7.1",
    "eslint": "^7.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.2",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "^25.5.1",
    "ts-loader": "^6.1.1",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.6.3",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  }
